/* 全局图形颜色 */
/*
报警的颜色
#f80000 红
#ffa101 黄
#0092fe 蓝
*/
$default-themes: (
  color1: #001e3dff,
  //左边栏
    color2: #204973ff,
  //上边
    color3: #092e55,
  //center 颜色
    color4: #07325e,
  // logo 下边横线
    color5: #022142,
  //标签颜色
    color6: #103760,
  //标签激活颜色
    color7: #0a53a0,
  //input 框
    color8: #007eff,
  //list title
    color9: #0e3e70,
  // list 颜色1
    color10: #0b4c90,
  //list 颜色2
    font_color1: #ffffff,
  btn_color1: #a47c42,
  btn_color2: red,
  border_color1: #0092fe
);

$default1-themes: ();

$themes: (
  default-theme: $default-themes,
  default1-themes: $default1-themes
);

@mixin color1() {
  @each $themename, $theme in $themes {
    [data-theme="#{$themename}"] & {
      background: map-get($map: $theme, $key: color1);
    }
  }
}
@mixin color2() {
  @each $themename, $theme in $themes {
    [data-theme="#{$themename}"] & {
      background: map-get($map: $theme, $key: color2);
    }
  }
}
@mixin color3() {
  @each $themename, $theme in $themes {
    [data-theme="#{$themename}"] & {
      background: map-get($map: $theme, $key: color3);
    }
  }
}
@mixin color4() {
  @each $themename, $theme in $themes {
    [data-theme="#{$themename}"] & {
      background: map-get($map: $theme, $key: color4);
    }
  }
}
@mixin color5() {
  @each $themename, $theme in $themes {
    [data-theme="#{$themename}"] & {
      background: map-get($map: $theme, $key: color5);
    }
  }
}
@mixin color6() {
  @each $themename, $theme in $themes {
    [data-theme="#{$themename}"] & {
      background: map-get($map: $theme, $key: color6);
    }
  }
}
@mixin color7() {
  @each $themename, $theme in $themes {
    [data-theme="#{$themename}"] & {
      background: map-get($map: $theme, $key: color7);
    }
  }
}
@mixin color8() {
  @each $themename, $theme in $themes {
    [data-theme="#{$themename}"] & {
      background: map-get($map: $theme, $key: color8);
    }
  }
}
@mixin color9() {
  @each $themename, $theme in $themes {
    [data-theme="#{$themename}"] & {
      background: map-get($map: $theme, $key: color9);
    }
  }
}
@mixin color10() {
  @each $themename, $theme in $themes {
    [data-theme="#{$themename}"] & {
      background: map-get($map: $theme, $key: color10);
    }
  }
}
@mixin font_color1() {
  @each $themename, $theme in $themes {
    [data-theme="#{$themename}"] & {
      color: map-get($map: $theme, $key: font_color1);
    }
  }
}
@mixin btn1_content() {
  @each $themename, $theme in $themes {
    [data-theme="#{$themename}"] & {
      background: map-get($map: $theme, $key: btn_color1);
    }
  }
}
@mixin btn2_content() {
  @each $themename, $theme in $themes {
    [data-theme="#{$themename}"] & {
      background: map-get($map: $theme, $key: btn_color2);
    }
  }
}
@mixin border_color1() {
  @each $themename, $theme in $themes {
    [data-theme="#{$themename}"] & {
      border: 1px solid map-get($map: $theme, $key: border_color1);
    }
  }
}
@mixin border_color2() {
  @each $themename, $theme in $themes {
    [data-theme="#{$themename}"] & {
      border: 1px solid map-get($map: $theme, $key: color7);
    }
  }
}
