/* 定义全局三块的颜色 */

.left {
  @include color1();
  @include font_color1();
}
.head {
  @include color2();
  @include font_color1();
}
.center {
  @include color3();
  @include font_color1();
}
/* 左边导航栏的颜色 */
.my-left-content {
  @include font_color1();
  .line {
    @include color4();
  }
}
/* 上边导航栏 */
.my-head-content {
  @include color1();
  .title {
    @include color3();
    .head_list_ul {
      .head_tab_li {
        @include color5();
      }
      .activity {
        @include color6();
      }
    }
  }
}
/* 搜索栏 背景色*/

.retrieval-bg-color-edge {
  @include color6();
  @include font_color1();
}

.title-search {
  /* 搜索栏 */
  .search-box {
    @include color6();
  }

  // 外部边框的样式
  .outsize-border {
    @include color7();
    @include border_color2();
    &:hover {
      @include border_color1();
    }
  }
  .outsize-border-color {
    @include border_color1();
  }
  /* 普通按钮 */
  .data-retrieval-btn-color {
    @include btn1_content();
    &:hover {
      @include btn2_content();
    }
  }
  .box-shadow {
    box-shadow: 0px 0px 7px 0px #1bb1ffcc;
  }
  .box-shadow2 {
    box-shadow: 0px 3px 14px -3px #1bb1ffcc;
  }
}

/* 重写下拉选框 */
.my-list-view {
  user-select: none;
  @include font_color1();
  @include border_color1();
  border-radius: 5px;
  .my-list-msg {
    background-color: #103760;
    color: #fff !important;
    &:hover {
      @include color2();
    }
    i {
      color: #fff;
    }
  }
}
/* 滚动条样式 */
.scroll-style-edge {
  ::-webkit-scrollbar {
    // display: none;
    width: 8px;
    height: 8px;
  }
  ::-webkit-scrollbar-track {
    border-radius: 3px;
    background: rgb(239, 239, 239);
  }
  //滚动条的轨道（里面装有Thumb）
  ::-webkit-scrollbar-thumb {
    border-radius: 10px;
    background: #0a53a0;
    // @include color7();
  }
  //滚动条的轨道的两端按钮，允许通过点击微调小方块的位置。
  ::-webkit-scrollbar-thumb:hover {
    @include color6();
  }
  // 火狐
  -ms-overflow-style: none;
  overflow: -moz-scrollbars-none;
}
/*  数据搜索框 */
.edge-content {
  .data-retrieval-bg {
    .data-retrieval-box {
      color: #ffffff;
      background-color: #103760;
      .flatpickr-input {
        background-color: #103760 !important;
        color: #fff;
      }
      .data-retrieval-title span {
        color: #ffffff;
      }
    }
  }
}
